			<RowDefinition/>
			<RowDefinition Height="40"/>
		</Grid.RowDefinitions>
		<Canvas x:Name="cnvView" Height="220" Background="{DynamicResource WindowBackgroundBrush}" Margin="0,0,11,0" VerticalAlignment="Top" HorizontalAlignment="Left">

