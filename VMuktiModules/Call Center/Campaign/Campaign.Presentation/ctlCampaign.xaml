						<Button  HorizontalAlignment="Center" VerticalAlignment="Center" Width="70" Height="25" Content="Select All" Canvas.Left="12" Canvas.Top="248" x:Name="btnSelectAllAGroups" Click="btnSelectAllAA_Click"/>
