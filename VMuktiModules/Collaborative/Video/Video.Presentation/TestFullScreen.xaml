    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="TestFullScreen" Height="768" Width="1024" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <Grid.RowDefinitions> 
            <RowDefinition Name="gdtop" Height="2"/>
            <RowDefinition Name="gdbottom" Height="*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="txtKey" Grid.Row="0"/>
        <Image Name="FSTpicUserVideo" VerticalAlignment="Stretch" Grid.Row="1" HorizontalAlignment="Stretch" Height="768" Width="1024" />
