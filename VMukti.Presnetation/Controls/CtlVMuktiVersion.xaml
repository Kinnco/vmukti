<UserControl x:Class="VMukti.Presentation.Controls.CtlVMuktiVersion"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Width="488" Height="416" Background="#FFFFFFFF">
    <Canvas Width="488" Height="416" >
        <Label Canvas.Left="12" Canvas.Top="23" Height="44" Name="lblHeader" Width="458" HorizontalContentAlignment="Center" FontSize="19">Select VMukti Version</Label>
        <CheckBox Canvas.Left="104" Canvas.Top="107" Height="16" Name="chVMuktiMeetingPlace" Width="137" IsChecked="True" IsEnabled="False">VMukti MeetingPlace</CheckBox>
        <CheckBox Canvas.Left="104" Canvas.Top="132" Height="16" Name="chVMuktiCallCenter" Width="120">VMukti CallCenter</CheckBox>
        <TextBlock Canvas.Left="12" Canvas.Top="162" Height="32" Name="txBoxNote" Width="458" TextWrapping="Wrap" HorizontalAlignment="Center">NOTE: Once you change VMukti version administrator of vmukti has to restart VMukti Server</TextBlock>
        <Button Canvas.Left="120" Canvas.Top="225" Height="23" Name="btnSave" Width="75" Click="btnSave_Click">Save</Button>
        <Button Canvas.Left="242" Canvas.Top="225" Height="23" Name="btnCancel" Width="75" Click="btnCancel_Click">Cancel</Button>
    </Canvas>
</UserControl>
